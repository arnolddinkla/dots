# Options.
auto-reload yes
reload-time 20
reload-threads 100
browser "surf"

# Keybinds.
unbind-key q
unbind-key ENTER

bind-key j down
bind-key k up
bind-key G end
bind-key g home
bind-key h quit
bind-key l open
bind-key U show-urls

# Colours.
color listnormal		white	default
color listfocus			black	white	bold
color listnormal_unread	red		default
color listfocus_unread	red		white	bold
color info				white	default
color article			white	default
